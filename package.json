  "bugs": {
    "url": "https://github.com/evfjunior/bootstrap4-starter-template/issues"
  },
  "homepage": "https://github.com/evfjunior/bootstrap4-starter-template#readme",
  "devDependencies": {
    "lite-server": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.32.8"
  },
  "dependencies": {
    "bootstrap": "^4.6.0"
  }
}
